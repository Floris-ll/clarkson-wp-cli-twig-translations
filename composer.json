{
  "name": "level-level/clarkson-wp-cli-twig-translations",
  "description": "Smart WP-CLI command to cache the Twig templates so they can be read by PoEdit",
  "type": "library",
  "license": "GPL-2.0-only",
  "authors": [
    {
      "name": "Level Level"
    }
  ],
  "minimum-stability": "stable",
  "require" : {
    "twig/twig": "^3.1",
    "twig/string-extra": "^3.1",
    "twig/intl-extra": "^3.1",
    "twig/html-extra": "^3.1",
    "twig/markdown-extra": "^3.1"
  },
  "autoload": {
      "psr-4": {
          "Clarkson\\WPCLITwigTranslations\\": "src/"
      },
      "files": ["clarkson-wp-cli-twig-translations.php"]
    },
  "require-dev": {
    "wp-cli/wp-cli": "^2.9"
  }
}
